{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/index.js"
      }
    ],
    "installCommand": "pnpm install --prod --no-frozen-lockfile",
    "buildCommand": "pnpm run build"
  }